# VARIABLES

STRING mdpConnexion <-- "formation" 
ENTIER itteration <-- 0


# DEBUT DU PROGRAMME

TANT QUE mdpConnexion != mdpUser ou itteration < 3
    ECRIRE "Quel est le mot de passe : "
    LIRE mdpUser
    iterration <-- iterration + 1
FIN TANT QUE

LIRE itteration

SI iterration >= 3
ALORS
    ECRIRE "Vôtre compte est bloqué"
SINON
    ECRIRE "Vous ête connecté !"
    itteration <-- 0
FIN SI


# FIN DU PROGRAMME